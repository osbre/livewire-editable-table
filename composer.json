{
    "name": "osbre/editable-table",
    "type": "library",
    "description": "",
    "keywords": [
        "laravel",
        "livewire",
        "livewire-component",
        "livewire-tables"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ostap B.",
            "email": "osbre@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "nunomaduro/collision": "^5.0",
        "pestphp/pest": "^0.1.5",
        "orchestra/testbench": "^5.3",
        "pestphp/pest-plugin-faker": "^0.1.1",
        "pestphp/pest-plugin-livewire": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ostap\\EditableTable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ostap\\EditableTable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ostap\\EditableTable\\EditableTableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
